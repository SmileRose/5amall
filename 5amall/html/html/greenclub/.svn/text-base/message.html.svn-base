		<script>
			$(function() {
				$(".item").hide();
				$(".item.current").show();
				$(".item-name a").each(function() {
					$(this).click(function() {
						var index = $(this).index();
						$(this).addClass("active");
						$(this).parent().siblings(".item").eq(index).show()
							.siblings(".item").hide();
					});
				});
			});
		</script>

