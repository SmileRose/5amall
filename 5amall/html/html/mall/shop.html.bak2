		<script>
			mui.init();
			var gallery = mui('.mui-slider');
			gallery.slider({
				interval: 5000 //自动轮播周期，若为0则不自动播放，默认为0；
			});
			mui.init({
				swipeBack: false,
			});
		</script>

		<script src="../../js/zepto.min.js"></script>
		<script src="../../js/touch.js"></script>
		<script type="text/javascript">
			
		
			$(function() {
				var findIt = $("#shop-list .mui-table-view-cell .goods-pic .mui-media-object");
				
				
				
				
				$(window).scroll(function() {
					//当内容滚动到底部时加载新的内容
					if ($(this).scrollTop() + $(window).height() + 20 >= $(document).height() && $(this).scrollTop() > +$(window).height()/2) {
						//图片变大
						$(findIt).addClass("current");
						
						//当前要加载的页码
						LoadPage();
					}
				});
			});

			function LoadPage() {
				var html =
					'<li class="mui-table-view-cell mui-media mui-col-xs-6">' 
						+ '<a href="item.html" title="">' 
							+'<div class="goods-pic">'
								+'<img class="mui-media-object" src="../../image/shop_05.png" alt="">'
							+'</div>'
							+ '<div class="mui-ellipsis-2 font12 mui-text-left">经典培根 培根肉片 西式烟楠片披萨炒饭1000g</div>' 
							+ '<div class="mui-text-left shop-price">' 
								+ '¥169' 
								+ '<strike>¥32</strike>' 
							+ '</div>'
						+ '</a>' 
					+ '</li>';
					
					
				$(html).appendTo("#shop-list");
			}
		</script>

